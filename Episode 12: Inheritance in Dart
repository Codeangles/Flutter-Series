void main() {
var a ; // a is an object
  var son = new Parent();
  print(son.land);
son.land = "48M";
print(son.land);
print(son.lake);
print(son.message());
}

class Parent extends GrandFather{
  var car = "15M";
  var land = "45M";
  var villa = "200M";

  String message(){
    return "Make this lake as a holiday place";
  }
}

class GrandFather{
   String message(){
     return "Give this lake to charity";
   }
  var lake = "500M";
}
