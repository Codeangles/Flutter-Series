// example 1

void main(){
 var a = {
 "zero": 10,
 "one": 20,
 "two":30,
 
 };
 
 // zero is key
 // 10 is values of key
 
 print(a["two"]);
 
 // a["two"] in maps same as a[2] in lists

}

